<%- |
  String  $command_name,
  String  $command,
  Boolean $sudo,
  String  $sudo_user,
  Stdlib::Absolutepath $libdir,
| -%>
# Generated by puppet
<%- if $command =~ /^\// { -%>
command[<%= $command_name %>]=<% if $sudo { %>/usr/bin/sudo -u <%= $sudo_user %> <% } %><%= $command %>
<%- } else { -%>
command[<%= $command_name %>]=<% if $sudo { %>/usr/bin/sudo -u <%= $sudo_user %> <% } %><%= $libdir %>/<%= $command %>
<%- } -%>
